alias Bd_SIGNAL B0
alias Bdbar_SIGNAL anti-B0
Alias      MyRho0       rho0
Alias      Myanti-Rho0  anti-Rho0
ChargeConj Myanti-Rho0  MyRho0
#
Decay Bd_SIGNAL
  1.000        MyRho0     mu+     mu-               PHSP;
Enddecay
CDecay Bdbar_SIGNAL
#
Decay MyRho0
  1.000        pi+        pi-                    VSS;
Enddecay
CDecay Myanti-Rho0
#
End
#
